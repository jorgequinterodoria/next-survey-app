generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model SurveyResponse {
  id               String   @id @default(cuid())
  consentName      String
  consentDoc       String
  consentAccepted  Boolean
  formType         String
  fichaData        Json
  intralaboralData Json
  extralaboralData Json
  estresData       Json
  filters          Json
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
}
